<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Main template -->
    <template id="report_medical_intervention">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="doc">
                <t t-call="web.external_layout">
                    <div class="page">
                        <h2>Report title</h2>
                        <p>This object's name is
                            <span t-field="doc.name"/>
                        </p>
                    </div>
                </t>
            </t>
        </t>
    </template>
</odoo>
