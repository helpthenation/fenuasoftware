<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Main template -->
    <template id="report_medical_information_document">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="doc">
                <t t-call="web.external_layout">
                    <div class="page">
                        <div class="row">
                            <h1>
                                <span t-esc="doc.title"/>
                            </h1>
                            <div>
                                <span t-esc="doc.content"/>
                            </div>
                        </div>
                    </div>
                </t>
            </t>
        </t>
    </template>
</odoo>