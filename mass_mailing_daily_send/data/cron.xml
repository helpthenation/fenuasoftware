<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="ir_cron_mass_mailing_queue_lot" model="ir.cron">
        <field name="name">Process Mass Mailing Daily</field>
        <field eval="True" name="active"/>
        <field name="user_id" ref="base.user_root"/>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="nextcall" eval="(DateTime.now()).strftime('%Y-%m-%d 18:00:00')"/>
        <field name="numbercall">-1</field>
        <field eval="False" name="doall"/>
        <field name="state">code</field>
        <field name="model_id" ref="mass_mailing.model_mail_mass_mailing"></field>
        <field name="code">model._process_mass_mailing_daily()</field>
    </record>
</odoo>