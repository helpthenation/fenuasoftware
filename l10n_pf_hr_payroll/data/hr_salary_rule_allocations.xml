<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
	<!-- Allocation -->
		<record id="hs25" model="hr.salary.rule">
			<field name="sequence" eval="2"/>
			<field name="name">Heures supplémentaire (25%)</field>
			<field name="category_id" ref="l10n_pf_hr_payroll.category_hs"/>
			<field name="code">HS25</field>
			<field name="active" eval="True"/>
			<field name="appears_on_payslip" eval="True"/>

			<!-- Conditions -->
			<field name="condition_select">python</field>
			<field name="condition_python">#Condition
result = worked_days.HS25
			</field>

			<!-- Calcul -->
			<field name="amount_select">percentage</field>
			<field name="amount_percentage_base">contract.wage</field>
			<field name="quantity">worked_days.HS25.number_of_hours</field>
			<field name="amount_percentage" eval="125"/>

			<!-- Contribution de la société -->
			<field name="register_id" ref=""/>
		</record>
	</data>
</odoo>